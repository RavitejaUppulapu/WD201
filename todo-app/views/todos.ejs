<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Due Date</th>
      <th>Completed</th>
    </tr>
  </thead>
  <tbody>
    <% for (var i=0; i < allTodos.length; i++) { %>
      <tr>
        <td><%= allTodos[i].title %></td>
        <td><%= allTodos[i].dueDate %></td>
        <td><%= allTodos[i].completed ? 'Yes' : 'No' %></td>
      </tr>
    <% } %>
  </tbody>
</table>
